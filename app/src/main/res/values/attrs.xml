<?xml version="1.0" encoding="utf-8"?>

<resources>
    <declare-styleable name="RGBLEDView">
        <attr name="enabled" format="boolean" />
        <attr name="isOn" format="boolean" />
        <attr name="size" format="integer" />
        <attr name="colorLight" format="integer" />
        <attr name="labelPosition" format="enum">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>

        <attr name="labelOff" format="string" />
        <attr name="labelOn" format="string" />
    </declare-styleable>

    <declare-styleable name="ValueView">
        <attr name="value" format="string" />
    </declare-styleable>

    <declare-styleable name="ShortNameView">
        <attr name="shortName" format="string" />
    </declare-styleable>

</resources>
